    <script>
        axios.get('/siteConfiguration/project/<%= projectName %>/getSiteConfiguration')
            .then(function (response) {
                console.log("hello ", response.data.data);
                document.getElementsByName('site_name')[0].value = response.data.data.siteName;
                document.getElementsByName('coin_name')[0].value = response.data.data.coinName;
                document.getElementsByName('token_supply')[0].value = response.data.data.tokenSupply;
                document.getElementsByName('soft_cap')[0].value = response.data.data.softCap;
                document.getElementsByName('hard_cap')[0].value = response.data.data.hardCap;
                document.getElementsByName("contract_address")[0].value = response.data.data.contractAddress;
                document.getElementsByName("contract_hash")[0].value = response.data.data.contractHash;
                document.getElementsByName("website_url")[0].value = response.data.data.homeURL;
                document.getElementById("imglogo").src = response.data.data.siteLogo;
                document.getElementsByName('start_date')[0].value =response.data.data.startDate;
                document.getElementsByName("end_date")[0].value = response.data.data.endDate;

            })
            .catch(function (e) {
                console.log(e);
            });
        $(document).ready(function () {
            var datetimeurl = document.getelementsbyname('datetime').value;
            var datetimeview = document.getelementsbyname('dateid');
            $.getJSON(datetimeurl, function (result) {
                datetimeview.innerText = result.date;
            });
            setInterval(function () {  // set Interval function to carry out same operation in the time specified
                $.getJSON(datetimeurl, function (result) {
                    datetimeview.innerText = result.date;
                });
            }, 6000);
        });

        

    </script>
